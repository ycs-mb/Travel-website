# Travel Photo Organization Workflow - Environment Variables

# API Keys (Optional - for production use with real VLM/LLM APIs)

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# LangSmith API Key (Optional - for observability and monitoring)
# Get from: https://smith.langchain.com/settings
# Used for tracing, monitoring, and evaluation of LLM workflows
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=travel-photo-workflow
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Workflow Configuration
# Override config.yaml settings with environment variables

# Input/Output Paths
INPUT_IMAGES_PATH=./sample_images
OUTPUT_DIR=./output

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Performance
PARALLEL_WORKERS=4
ENABLE_CACHE=true
